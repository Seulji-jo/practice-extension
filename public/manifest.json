{
  "short_name": "React Extension",
  "name": "Routing in Chrome Extension written in React",
  "manifest_version": 3,
  "version": "1.0",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-36.png",
      "48": "icon-48.png",
      "128": "icon-120.png"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-36.png",
    "48": "icon-48.png",
    "128": "icon-120.png"
  }
}
